{
  "name": "rsuite-theme",
  "version": "3.0.0-next.7",
  "description": "The suite theme for pagurian",
  "main": "main.js",
  "bin": {
    "rsuite-theme": "rsuite-theme.js"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/rsuite/rsuite-theme.git"
  },
  "scripts": {
    "test": "npm run prepublish && rm -rf dist/test/ && rm -rf coverage && jest --silent --coverage",
    "coveralls": "cat ./coverage/lcov.info | ./node_modules/coveralls/bin/coveralls.js",
    "build": "babel src/script --out-dir lib && babel src/bin --out-dir bin",
    "prepublish": "gulp && npm run build"
  },
  "keywords": [
    "css",
    "bootstrap",
    "pagurian"
  ],
  "author": [
    {
      "name": "Godfery Yang",
      "email": "hiyangguo@qq.com",
      "url": "http://hiyangguo.github.io"
    }
  ],
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/rsuite/rsuite-theme/issues"
  },
  "homepage": "https://github.com/rsuite/rsuite-theme",
  "dependencies": {
    "async": "^2.5.0",
    "colors": "^1.1.2",
    "commander": "^2.9.0",
    "fs-extra": "^0.30.0",
    "glob": "^7.0.6",
    "less": "^2.7.1"
  },
  "devDependencies": {
    "autoprefixer": "^7.2.1",
    "babel-cli": "^6.14.0",
    "babel-preset-env": "^1.6.1",
    "coveralls": "^3.0.0",
    "cssnano": "^3.10.0",
    "gulp": "^3.9.1",
    "gulp-clean": "^0.3.2",
    "gulp-less": "^3.3.2",
    "gulp-postcss": "^7.0.0",
    "gulp-rename": "^1.2.2",
    "gulp-sourcemaps": "^2.6.1",
    "jest": "^21.2.1"
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "ie >= 9"
  ],
  "files": [
    "bin",
    "lib",
    "LICENSE",
    "main.js",
    "README.md",
    "rsuite-theme.js"
  ]
}
